@model List<Grillbot.Database.Entity.Views.SummarizedCommandLog>;

@{
    ViewData["Title"] = "Kompletní statistiky volání";
}

<link rel="stylesheet" href="~/css/callStats.css" />
<table class="top-space table">
    <thead>
        <tr>
            <th>Skupina</th>
            <th>Příkaz</th>
            <th>Počet volání</th>
        </tr>
    </thead>
    <tbody>
        @foreach(var item in Model)
        {
            <tr>
                <td>@item.Group</td>
                <td>@item.Command</td>
                <td>@FormatHelper.FormatWithSpaces(item.Count)</td>
            </tr>
        }
    </tbody>
</table>